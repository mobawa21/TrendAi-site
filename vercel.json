{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "terms.html",
      "use": "@vercel/static"
    },
    {
      "src": "privacy.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/terms",
      "dest": "/terms.html"
    },
    {
      "src": "/privacy",
      "dest": "/privacy.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

